{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "useBuiltins": false,
        "development": true,
        "throwIfNamespace": true
      },
      "optimizer": {
        "globals": {
          "vars": {
            "__DEBUG__": "true"
          }
        }
      }
    },
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "app/*": ["./src/app/*"],
      "server/*": ["./src/server/*"],
      "static/*": ["./src/static/*"],
      "test/*": ["./test/*"]
    },
    "target": "es2020",
    "loose": false,
    "externalHelpers": false
  },
  "minify": true
}
